sdsf d xcf,pathin,device=all

* xcf signalling pathout status
sdsf d xcf,pathout

* xcf signalling pathout status
sdsf d xcf,pathout,device=all

* displays all policy infomation
sdsf d xcf,policy

* displays specific policy info
sdsf d xcf,policy,type

* displays all systems in sysplex
sdsf d xcf,sysplex all

* displays all structures
sdsf d xcf,str=strm=all

* displays list of cfrm policies
sdsf d xcf,structure

* Display lost connections
sdsf d xcf,str,status=lossconn

* ISKLM commands
sdsf F ISKLMSRV,APPL='LIST -KEY'
sdsf F ISKLMSRV,APPL='LIST -CERT'
sdsf F ISKLMSRV,APPL='LIST -KEYSYM'
sdsf F ISKLMSRV,APPL='LISTCERTS'

* ICSF Display Commands (requires ICSF HCR77C1 FMID)
sdsf  D ICSF,LIST
sdsf  D ICSF,LIST,SYSPLEX=NO
sdsf  D ICSF,LIST,SYSPLEX=YES
sdsf  D ICSF,CARDS
sdsf  D ICSF,KDS
sdsf  D ICSF,MKS
sdsf  D ICSF,OPT
sdsf  D ICSF,REMOTE

* ACF/VTAM Display Commands
sdsf d net,vtamopts
sdsf d net,bfruse
sdsf d net,appls
sdsf d net,pathtab
sdsf d net,majnotes

* Update APF List dynamically
sdsf setprog apf,add,dsname=&dsn,volume=&vol

* Replace Linklist - use some or all of these
sdsf d prog,lnklst
sdsf setprog lnklst define name(&newlnk) copyfrom(&oldlnk)
sdsf setprog lnklst add name(&newlnk) dsname(&dsnadd) vol(&voladd)
sdsf setprog lnklst delete name(&newlnk) dsname(&dsnkill)
sdsf setprog lnklst activate name(&newlnk)

* Update Dynamic LPA
sdsf setprog lpa,add,modname=&lmod,dsname=&dsn
sdsf setprog lpa,delete,modname=&lmod

* CSSMTP Commands
sdsf f cssmtp,display,loglevel
sdsf f cssmtp,loglevel,level=&loglevel

* Health Check Display
sdsf F HZSPROC,DISPLAY STATUS

* System Rexx
sdsf f axr,sysrexx,st d r


